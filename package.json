{
  "name": "high-low-jack",
  "version": "0.0.1",
  "description": "An engine for the card game `high-low-jack`.",
  "main": "compiled/src/match.js",
  "types": "compiled/src/match.js",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "test": "tsc && ava",
    "start": "tsc && browserify compiled/ui/index.js -o high-low-jack-ui/bundled.js && open high-low-jack-ui/index.html"
  },
  "dependencies": {
    "lodash": "^4.17.11",
    "typescript": "^3.4.2"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.123",
    "@types/node": "^11.13.0",
    "@typescript-eslint/eslint-plugin": "^1.6.0",
    "@typescript-eslint/parser": "^1.6.0",
    "ava": "^1.4.1",
    "browserify": "^16.2.3",
    "eslint": "^5.16.0"
  },
  "ava": {
    "files": [
      "./compiled/engine/tests/*.js"
    ],
    "failFast": true,
    "tap": true
  },
  "keywords": [
    "high-low-jack",
    "high",
    "low",
    "jack",
    "card",
    "game"
  ],
  "author": "Ryan Casperson <casperson.ryan@gmail.com>",
  "repository": "https://github.com/rbcasperson/high-low-jack",
  "license": "MIT"
}
